locals.
	overwritten: (index) Before 'extends'
	page:
		title: This is index
		description: A beautiful index page

extends(src='layout.sgr')
	locals.
		overwritten: (index) Inside 'extends'

	block(name='title')
		locals.
			overwritten: (index) Inside 'title' block
		title {{ page.title }}

	block(name='content')
		p {{ overwritten }}
