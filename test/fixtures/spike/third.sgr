locals.
	overwritten: (third) Before 'extends'
	page:
		title: A third page
		description: The third page has even more information

extends(src='layout.sgr')
	locals.
		overwritten: (third) Inside 'extends'

	block(name='title')
		locals.
			overwritten: (third) Inside 'title' block
		title {{ (item || page).title }}

	block(name='content')
		p {{ overwritten }}
